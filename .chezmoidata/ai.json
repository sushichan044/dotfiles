{
  "claude": {
    "settings": {
      "mcpServers": {
        "bundlephobia": {
          "type": "http",
          "url": "https://bundlephobia-mcp.sushichan044.workers.dev/mcp"
        },
        "cloudflare-docs": {
          "type": "http",
          "url": "https://docs.mcp.cloudflare.com/mcp"
        },
        "context7": {
          "type": "http",
          "url": "https://mcp.context7.com/mcp"
        },
        "deepwiki": {
          "type": "http",
          "url": "https://mcp.deepwiki.com/mcp"
        },
        "devin": {
          "type": "http",
          "url": "https://mcp.devin.ai/mcp",
          "headers": {
            "Authorization": "Bearer DEVIN_API_KEY"
          }
        },
        "notion": {
          "type": "http",
          "url": "https://mcp.notion.com/mcp"
        },
        "readability": {
          "args": [
            "run",
            "--allow-env",
            "--allow-net",
            "--allow-read",
            "--allow-run",
            "npm:@mizchi/readability@0.6.8",
            "--",
            "--mcp"
          ],
          "command": "deno"
        },
        "sentry": {
          "type": "http",
          "url": "https://mcp.sentry.dev/mcp"
        },
        "sequential-thinking": {
          "args": [
            "run",
            "--allow-env",
            "npm:@modelcontextprotocol/server-sequential-thinking@latest"
          ],
          "command": "deno"
        }
      },
      "preferredNotifChannel": "terminal_bell",
      "theme": "dark"
    }
  },
