        },
        "sequential-thinking": {
          "args": [
            "x",
            "deno",
            "--",
            "deno",
            "run",
            "--allow-env",
            "npm:@modelcontextprotocol/server-sequential-thinking@latest"
          ],
          "command": "mise",
          "type": "stdio"
        }
      }
    }
  },
