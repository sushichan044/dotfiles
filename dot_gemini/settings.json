{
  "context": {
    "fileName": ["AGENTS.md", "GEMINI.md"]
  },
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "ide": {
    "enabled": true,
    "hasSeenNudge": true
  },
  "mcpServers": {
    "aws-knowledge": {
      "httpUrl": "https://knowledge-mcp.global.api.aws",
      "trust": true
    },
    "bundlephobia": {
      "httpUrl": "https://bundlephobia-mcp.sushichan044.workers.dev/mcp",
      "trust": true
    },
    "cloudflare-docs": {
      "httpUrl": "https://docs.mcp.cloudflare.com/mcp",
      "trust": true
    },
    "context7": {
      "httpUrl": "https://mcp.context7.com/mcp",
      "trust": true
    },
    "deepwiki": {
      "httpUrl": "https://mcp.deepwiki.com/mcp",
      "trust": true
    },
    "grep": {
      "httpUrl": "https://mcp.grep.app",
      "trust": true
    },
    "mcpdoc": {
      "command": "uvx",
      "args": [
        "--from",
        "mcpdoc",
        "mcpdoc",
        "--json",
        "${HOME}/ai/config/mcpdoc.json",
        "--transport",
        "stdio"
      ]
    },
    "readability": {
      "args": [
        "run",
        "--allow-env",
        "--allow-net",
        "--allow-read",
        "--allow-run",
        "npm:@mizchi/readability@0.6.8",
        "--",
        "--mcp"
      ],
      "command": "deno"
    },
    "repomix": {
      "args": [
        "run",
        "--allow-env",
        "--allow-net=github.com",
        "--allow-run=sh",
        "--allow-sys",
        "--allow-read",
        "npm:repomix@latest",
        "--mcp"
      ],
      "command": "deno"
    },
    "sequential-thinking": {
      "args": [
        "run",
        "--allow-env",
        "npm:@modelcontextprotocol/server-sequential-thinking@latest"
      ],
      "command": "deno"
    }
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  "telemetry": {
    "enabled": false
  },
  "tools": {
    "sandbox": true
  },
  "ui": {
    "theme": "Atom One"
  }
}
